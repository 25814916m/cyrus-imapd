# Authorization namespace.  Two choices:
#
# Kerberos
AUTH=auth_krb.o kerberos.o
# Unix passwd file
#AUTH=auth_unix.o

# ACL interpretation.  Only one choice for now:
ACL=acl_afs.o

OPT=-g

OBJS= acl.o assert.o charset.o glob.o util.o n_binsearch.o xmalloc.o \
	$(ACL) $(AUTH)

CFLAGS=$(OPT) $(DEFINES) -I/usr/local/include

all: libcyrus.a

libcyrus.a: $(OBJS)
	ar cr libcyrus.a $(OBJS)
	ranlib libcyrus.a

clean:
	rm -f *.o *.a Makefile.bak

depend:
	makedepend $(CFLAGS) *.c

