srcdir = @srcdir@
VPATH = @srcdir@

CC = @CC@
YACC = @YACC@
YFLAGS = -d
LEX = @LEX@
RANLIB = @RANLIB@

DEFS = @DEFS@
CPPFLAGS = -I. -I$(srcdir) -I$(srcdir)/../lib @CPPFLAGS@
CFLAGS = @CFLAGS@
LDFLAGS = @LDFLAGS@

MAKEDEPEND = @MAKEDEPEND@

TIMSIEVED_LIBS = @TIMSIEVED_LIBS@

IMAP_LIBS = @IMAP_LIBS@
LIBS = 
DEPLIBS=../lib/libcyrus.a ../sieve/libsieve.a @DEPLIBS@

all: timsieved

install:

OBJS=actions.o config.o lex.o mystring.o scripttest.o token.o timsieved.o parse.tab.o

timsieved: $(OBJS) $(SIEVE_LIBS)
	$(CC) $(LDFLAGS) -o timsieved $(OBJS) $(DEPLIBS) $(IMAP_LIBS)

clean:
	rm -f *.o *.a Makefile.bak makedepend.log

distclean: clean
	rm -f Makefile

depend:
	${MAKEDEPEND} $(CPPFLAGS) $(DEFS) -I$(srcdir) $(CFLAGS) *.c $(srcdir)/*.c 1>makedepend.log 2>&1

.c.o:
	$(CC) -c $(CPPFLAGS) $(DEFS) -I$(srcdir) -I. $(CFLAGS) \
	$<

