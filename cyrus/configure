s%@CPP@%$CPP%g
s%@AWK@%$AWK%g
s%@LIBOBJS@%$LIBOBJS%g
s%@PRE_SUBDIRS@%$PRE_SUBDIRS%g
s%@EXTRA_SUBDIRS@%$EXTRA_SUBDIRS%g
s%@DEPLIBS@%$DEPLIBS%g
s%@WITH_NONBLOCK@%$WITH_NONBLOCK%g
s%@WITH_GMTOFF@%$WITH_GMTOFF%g
