MAKE=gmake

WITHAUTH="--with-auth=krb_pts"
if test `sys` = pmax_ul4; then
	WITHAUTH=""
fi

LNCOMMAND="${SRCDIR}/configure $WITHAUTH --with-inn=/usr/news --enable-amssync --enable-cmulocal --with-cyrus-user=$USER --with-news-group=staff"
