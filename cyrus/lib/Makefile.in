kerberos.o: /usr/include/sys/param.h /usr/include/machine/param.h
kerberos.o: /usr/include/machine/devaddr.h /usr/include/sys/signal.h
kerberos.o: /usr/include/vm/faultcode.h /usr/include/netinet/in.h
kerberos.o: /usr/local/include/krb.h /usr/local/include/mit-copyright.h
kerberos.o: /usr/local/include/des.h /usr/local/include/mit-cpyright.h
kerberos.o: kerberos.h
n_binsearch.o: /usr/include/sys/param.h /usr/include/machine/param.h
n_binsearch.o: /usr/include/machine/devaddr.h /usr/include/sys/signal.h
n_binsearch.o: /usr/include/vm/faultcode.h /usr/include/sys/file.h
