ACL=acl_afs.o
AUTH=auth_krb.o

OPT=-g

OBJS= acl.o glob.o kerberos.o util.o n_binsearch.o $(ACL) $(AUTH)

CFLAGS=$(OPT) $(DEFINES) -I/usr/local/include

all: libcyrus.a

libcyrus.a: $(OBJS)
	ar cr libcyrus.a $(OBJS)
	ranlib libcyrus.a

clean:
	rm -f *.o *.a

depend:
	makedepend $(CFLAGS) *.c

# DO NOT DELETE THIS LINE -- make depend depends on it.

acl.o: /usr/include/stdio.h acl.h
glob.o: /usr/include/stdio.h /usr/include/ctype.h util.h glob.h
kerberos.o: /usr/include/stdio.h /usr/include/string.h
